{
  "name": "scss-sassdoc-parser",
  "version": "1.0.4",
  "engines": {
    "node": ">=14"
  },
  "description": "A more lightweight parser for SassDoc in SCSS files, with TypeScript definitions.",
  "keywords": [
    "sassdoc",
    "scss",
    "sass"
  ],
  "homepage": "https://github.com/wkillerud/sassdoc-parser#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/wkillerud/sassdoc-parser.git"
  },
  "bugs": {
    "url": "https://github.com/wkillerud/sassdoc-parser"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run compile",
    "compile": "tsc",
    "dev": "tsc -w",
    "lint": "eslint 'src/**/*.ts'",
    "test": "jest",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "prepublishOnly": "npm run build && npm run lint && npm run test"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix"
    ],
    "src/**/*.{md,ts,json,css}": [
      "prettier --write"
    ]
  },
  "author": "William Killerud <william@killerud.com> (https://www.somewhatstationery.dev/)",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/lodash.uniq": "^4.5.7",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "lodash.uniq": "^4.5.0",
    "scss-comment-parser": "^0.8.4",
    "strip-indent": "^3.0.0"
  }
}
